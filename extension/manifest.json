{
	"name": "AnkiTab",
	"version": "0.1",
	"description": "Replace the new tab with an anki window",
	"manifest_version": 2,
	"incognito": "split",
	"browser_action": {
		"default_icon": {
			"16": "icons/16.png",
			"24": "icons/24.png",
			"32": "icons/32.png"
		},
		"default_title": "AnkiTab"
	},
	"icons": {
		"16": "icons/16.png",
		"48": "icons/48.png",
		"128": "icons/128.png"
	},
	"permissions": [
		"cookies",
		"https://ankiuser.net/*",
		"https://ankiweb.net/*"
	],
	"chrome_url_overrides" : {
		"newtab": "app/index.html"
	}

}
